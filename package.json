{
  "name": "convention-bingo",
  "version": "1.4.1",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/Rankarusu/ConBingo3.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --mode=release",
    "description": "A bullshit-bingo-esque to play with your weeb friends at conventions",
    "ios": "react-native run-ios",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "start": "react-native start",
    "start:clean": "react-native start --reset-cache",
    "gradle:clean": "cd android && ./gradlew clean && cd ..",
    "test": "jest",
    "release": "semantic-release",
    "postversion": "react-native-version --never-amend",
    "postinstall": "patch-package",
    "create-appicon": "iconset create -A -b ./assets/logo_bg.png -f ./assets/logo_fg.png ./assets/logo_full.png"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.1",
    "@react-native-community/hooks": "^3.0.0",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/stack": "^6.3.17",
    "@reduxjs/toolkit": "^1.9.5",
    "patch-package": "^8.0.0",
    "react": "18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-native": "^0.72.3",
    "react-native-animated-pagination-dots": "^0.1.73",
    "react-native-confetti-cannon": "^1.5.2",
    "react-native-document-picker": "^9.0.1",
    "react-native-exception-handler": "^2.10.10",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.12.1",
    "react-native-logs": "^5.0.1",
    "react-native-paper": "^5.10.0",
    "react-native-reanimated": "^3.4.2",
    "react-native-safe-area-context": "^4.7.1",
    "react-native-screens": "^3.24.0",
    "react-native-share": "^9.2.3",
    "react-native-vector-icons": "^10.0.0",
    "react-redux": "^8.1.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/runtime": "^7.22.10",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@commitlint/cz-commitlint": "^17.6.7",
    "@react-native/metro-config": "^0.72.9",
    "@semantic-release/git": "^10.0.1",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.3",
    "@types/react": "^18.2.19",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.2",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "icon-set-creator": "^1.1.5",
    "jest": "^29.6.2",
    "metro-react-native-babel-preset": "^0.76.8",
    "prettier": "^3.0.1",
    "react-native-version": "^4.0.0",
    "react-test-renderer": "18.2.0",
    "semantic-release": "^21.0.7",
    "typescript": "^5.1.6"
  },
  "jest": {
    "preset": "react-native"
  },
  "engines": {
    "node": ">=16"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}